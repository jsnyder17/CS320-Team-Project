<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://JAVA.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  <servlet>
    <servlet-name>IndexServlet</servlet-name>
    <servlet-class>nullpointerexception.tbag.servlet.IndexServlet</servlet-class>
  </servlet>    
  <servlet>
    <servlet-name>GameServlet</servlet-name>
    <servlet-class>nullpointerexception.tbag.servlet.GameServlet</servlet-class>
  </servlet>    
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<servlet-class>nullpointerexception.tbag.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>YouLoseServlet</servlet-name>
  	<servlet-class>nullpointerexception.tbag.servlet.YouLoseServlet</servlet-class></servlet>

  <!-- Servlet mappings -->
  <servlet-mapping>
    <servlet-name>IndexServlet</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>     
  <servlet-mapping>
    <servlet-name>GameServlet</servlet-name>
    <url-pattern>/game</url-pattern>
  </servlet-mapping>     
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>YouLoseServlet</servlet-name>
  	<url-pattern>/bluekid</url-pattern>
  	<url-pattern>/youdied</url-pattern>
  	<url-pattern>/mayor</url-pattern>
  	<url-pattern>/chariots</url-pattern>
  	<url-pattern>/frogg</url-pattern>
  	<url-pattern>/frogg2</url-pattern>
  	<url-pattern>/therules</url-pattern>
  	<url-pattern>/tyler</url-pattern>
  	<url-pattern>/nicejacket</url-pattern>
  	<url-pattern>/bobby</url-pattern>
  	<url-pattern>/patrick</url-pattern>
  	<url-pattern>/emoji</url-pattern></servlet-mapping>

  <!--
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>       
	-->
</web-app>